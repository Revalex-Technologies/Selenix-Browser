{
  "name": "selenix",
  "version": "1.0.0",
  "packageManager": "yarn@4.9.2",
  "sideEffects": false,
  "description": "Extensible, fast and innovative web browser with material UI.",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "homepage": "https://github.com/VQMSoftware/Selenix-Browser#readme",
  "main": "build/main.bundle.js",
  "author": "VQMSoftware <vqmsoftware@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vqmsoftware/Selenix-Browser.git"
  },
  "bugs": {
    "url": "https://github.com/VQMSoftware/Selenix-Browser/issues"
  },
  "scripts": {
    "dev-renderer": "cross-env DEV=1 webpack serve --config webpack.config.renderer.js",
    "dev-webpack": "cross-env DEV=1 webpack",
    "build-renderer": "cross-env webpack --config webpack.config.renderer.js",
    "dev": "cross-env START=1 npm run watch",
    "build": "rimraf build && concurrently \"npm run build-renderer\" \"cross-env webpack\"",
    "ci-build": "node scripts/ci-build.js",
    "start": "electron .",
    "watch": "concurrently \"npm run dev-renderer\" \"npm run dev-webpack\"",
    "compile-win32": "npm run build && electron-builder -w",
    "compile-darwin": "npm run build && electron-builder -m",
    "compile-linux": "npm run build && electron-builder -l",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "rebuild": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@electron/remote": "^2.1.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@types/animejs": "^3.1.2",
    "@types/chrome": "0.1.3",
    "@types/crypto-js": "^4.0.1",
    "@types/jszip": "^3.4.1",
    "@types/nedb": "1.8.11",
    "@types/node": "^24.0.0",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/rimraf": "^3.0.0",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "@wexond/rpc-core": "^1.0.3",
    "@wexond/rpc-electron": "^1.0.3",
    "animejs": "^3.2.1",
    "awesome-node-loader": "^1.1.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "7.0.3",
    "cross-fetch": "^4.1.0",
    "electron": "^37.2.6",
    "electron-builder": "26.0.12",
    "electron-extensions": "^7.0.0-beta.3",
    "electron-store": "10.1.0",
    "electron-updater": "6.6.2",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-webpack-plugin": "^5.1.0",
    "icojs": "^0.16.0",
    "jszip": "^3.6.0",
    "mobx": "6.1.7",
    "mobx-react-lite": "3.2.0",
    "nedb": "1.8.0",
    "node-bookmarks-parser": "^2.0.0",
    "node-fetch": "^2.6.1",
    "prettier": "2.2.1",
    "pretty-bytes": "5.5.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "^17.0.0",
    "react-refresh": "^0.10.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.2.1",
    "terser": "^5.43.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^5.9.2",
    "typescript-plugin-styled-components": "^3.0.0",
    "webpack": "^5.101.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@ghostery/adblocker-electron": "^2.11.3",
    "@ghostery/adblocker-electron-preload": "^2.11.3"
  }
}
